    <descriptions defaultDescription=""/>
    <variableProperties datatype="NVARCHAR" length="3" mandatory="true">
      <valueDomain type="empty"/>
      <selection multiLine="false" type="Single"/>
    </variableProperties>
  </variable>
</localVariables>
<variableMappings/>
<dataSources/>
<calculationViews>
  <calculationView xsi:type="Calculation:GraphView" id="Graph_1" workspace="ROUTES" action="PATTERN_MATCHING">
    <viewAttributes>
      <viewAttribute datatype="NVARCHAR" length="3" id="IATA3">
        <descriptions defaultDescription="IATA3"/>
      </viewAttribute>
      <viewAttribute datatype="NVARCHAR" length="3" id="IATA3_1">
        <descriptions defaultDescription="IATA3_1"/>
      </viewAttribute>
      <viewAttribute datatype="NVARCHAR" length="3" id="IATA3_2">
        <descriptions defaultDescription="IATA3_2"/>
      </viewAttribute>
      <viewAttribute datatype="NVARCHAR" length="8" id="ID">
        <descriptions defaultDescription="ID"/>
      </viewAttribute>
      <viewAttribute datatype="INTEGER" id="DISTANCE" aggregationType="sum">
        <descriptions defaultDescription="DISTANCE"/>
      </viewAttribute>
      <viewAttribute datatype="NVARCHAR" length="2" id="AIRLINE_IATA2">
        <descriptions defaultDescription="AIRLINE_IATA2"/>
      </viewAttribute>
      <viewAttribute datatype="NVARCHAR" length="8" id="ID_1">
        <descriptions defaultDescription="ID_1"/>
      </viewAttribute>
      <viewAttribute datatype="INTEGER" id="DISTANCE_1" aggregationType="sum">
        <descriptions defaultDescription="DISTANCE_1"/>
      </viewAttribute>
      <viewAttribute datatype="NVARCHAR" length="2" id="AIRLINE_IATA2_1">
        <descriptions defaultDescription="AIRLINE_IATA2_1"/>
      </viewAttribute>
    </viewAttributes>
    <calculatedViewAttributes/>
    <restrictedViewAttributes/>
    <patternMatching>
      <vertexVariable name="Start"/>
      <vertexVariable name="Destination"/>
      <vertexVariable name="ChangeAt"/>
      <edgeVariable name="Leg1" sourceVariable="Start" targetVariable="ChangeAt"/>
      <edgeVariable name="Leg2" sourceVariable="ChangeAt" targetVariable="Destination"/>
      <mapping variable="Start" element="IATA3" target="IATA3"/>
      <mapping variable="ChangeAt" element="IATA3" target="IATA3_1"/>
      <mapping variable="Destination" element="IATA3" target="IATA3_2"/>
      <mapping variable="Leg1" element="ID" target="ID"/>
      <mapping variable="Leg1" element="DISTANCE" target="DISTANCE"/>
      <mapping variable="Leg1" element="AIRLINE_IATA2" target="AIRLINE_IATA2"/>
      <mapping variable="Leg2" element="ID" target="ID_1"/>
      <mapping variable="Leg2" element="DISTANCE" target="DISTANCE_1"/>
      <mapping variable="Leg2" element="AIRLINE_IATA2" target="AIRLINE_IATA2_1"/>
      <operation xsi:type="graph:CompoundOperation" operator="AND">
        <operation xsi:type="graph:PredicateOperation" operator="EQ">
          <argument>
            <variableElement variable="Start" element="IATA3"/>
          </argument>
          <argument>
            <parameter name="airportFrom" datatype="NVARCHAR"/>
          </argument>
        </operation>
        <operation xsi:type="graph:PredicateOperation" operator="EQ">
          <argument>
            <variableElement variable="Destination" element="IATA3"/>
          </argument>
          <argument>
            <parameter name="airportTo" datatype="NVARCHAR"/>
          </argument>
        </operation>
      </operation>
    </patternMatching>
  </calculationView>
  <calculationView xsi:type="Calculation:ProjectionView" id="Projection_1">
    <viewAttributes>
      <viewAttribute id="IATA3">
        <descriptions defaultDescription="IATA3"/>
      </viewAttribute>
      <viewAttribute id="IATA3_1">
        <descriptions defaultDescription="IATA3_1"/>
      </viewAttribute>
      <viewAttribute id="IATA3_2">
        <descriptions defaultDescription="IATA3_2"/>
      </viewAttribute>
      <viewAttribute id="ID">
        <descriptions defaultDescription="ID"/>
      </viewAttribute>
      <viewAttribute id="DISTANCE">
        <descriptions defaultDescription="DISTANCE"/>
      </viewAttribute>
      <viewAttribute id="AIRLINE_IATA2">
        <descriptions defaultDescription="AIRLINE_IATA2"/>
      </viewAttribute>
      <viewAttribute id="ID_1">
        <descriptions defaultDescription="ID_1"/>
      </viewAttribute>
      <viewAttribute id="DISTANCE_1">
        <descriptions defaultDescription="DISTANCE_1"/>
      </viewAttribute>
      <viewAttribute id="AIRLINE_IATA2_1">
        <descriptions defaultDescription="AIRLINE_IATA2_1"/>
      </viewAttribute>
    </viewAttributes>
    <calculatedViewAttributes>
      <calculatedViewAttribute id="routeText" datatype="NVARCHAR" length="50" expressionLanguage="SQL">
        <formula>CONCAT(
  CONCAT(
    CONCAT(
      CONCAT(
        &quot;IATA3&quot;
      , ' -&gt; '
      )
    , &quot;IATA3_1&quot;
    )
  , ' -&gt; '
  )
, &quot;IATA3_2&quot;
)</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="airlineText" datatype="NVARCHAR" length="40" expressionLanguage="SQL">
        <formula>CONCAT(
  CONCAT(
    &quot;AIRLINE_IATA2&quot;
  , ', '
  )
, &quot;AIRLINE_IATA2_1&quot;
)</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="totalDistance" datatype="INTEGER" expressionLanguage="SQL">
        <formula>&quot;DISTANCE&quot; + &quot;DISTANCE_1&quot;</formula>
      </calculatedViewAttribute>
    </calculatedViewAttributes>
    <restrictedViewAttributes/>
    <input node="Graph_1">
      <mapping xsi:type="Calculation:AttributeMapping" target="IATA3" source="IATA3"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="IATA3_1" source="IATA3_1"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="IATA3_2" source="IATA3_2"/>
