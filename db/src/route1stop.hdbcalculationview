      <mapping xsi:type="Calculation:AttributeMapping" target="DISTANCE_1" source="DISTANCE_1"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="AIRLINE_IATA2_1" source="AIRLINE_IATA2_1"/>
    </input>
  </calculationView>
</calculationViews>
